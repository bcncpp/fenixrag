# =============================================================================
# FILE: .env.example
# =============================================================================
# Copy this to .env and fill in your values

# Google AI API Key (required for Gemini embeddings)
GOOGLE_API_KEY=your_google_api_key_here

# Database Configuration
DATABASE_URL=postgresql+psycopg2://postgres:password@localhost:5432/gemini_docs_db
ASYNC_DATABASE_URL=postgresql+asyncpg://postgres:password@localhost:5432/gemini_docs_db

# Vector Configuration
VECTOR_DIMENSION=768
COLLECTION_NAME=gemini_documents

# Gemini Model Configuration
GEMINI_MODEL=models/embedding-001

# Application Settings
LOG_LEVEL=INFO
MAX_BATCH_SIZE=100

